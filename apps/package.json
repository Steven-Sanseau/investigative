  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "android": "expo start --android",
    "build": "next build",
    "build:web": "next build && next export",
    "deploy:prod": "deploy:web && now --prod",
    "deploy:web": "yarn run predeploy:web && now",
    "eject": "expo eject",
    "format": "prettier --write \"**/*.{json,md,ts}\"",
    "generate": "graphql-codegen --config codegen.yml && replace-in-file \"1536X1536 =\" \"'1536X1536' =\" src/generated/graphql.ts && replace-in-file \"2048X2048 =\" \"'2048X2048' =\" src/generated/graphql.ts",
    "ios": "expo start --ios",
    "lint": " yarn eslint ./src --ext .js,.jsx,.ts,.tsx",
    "next": "next dev",
    "optimize": "rm -rf out; npx expo-optimize",
    "predeploy:web": "yarn run optimize && yarn run build:web",
    "prenext": "yarn generate",
    "serve": "yarn build:web && serve -p 3000 ./out",
    "start": "expo start",
    "web": "expo start --web"
  }
}